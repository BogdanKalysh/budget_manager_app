 width: 140px;
 border-radius: 25px;
 background: transparent;
 border: 1px solid white;
 font-size: 20px;
}

#logoutButton{
 height: 50px;
 width: 140px;
 border-radius: 25px;
 background: rgb(237, 106, 94);
 font-size: 20px;
}

#saveButton::hover{
 background: rgb(88, 153, 255);
 border-style: none;
}

#deleteCategoryButton::hover{
	background: rgb(237, 106, 94);
	border-style: none;
}

#categoriesComboBox{
 	height: 50px;
 	width: 200px;
 	border-radius: 25px;
 	background: transparent;
 	border: 1px solid white;
	font-size: 20px;
}

#categoriesComboBox::drop-down{
	width: 40px;
	height: 20px;
	subcontrol-position: right;
}
